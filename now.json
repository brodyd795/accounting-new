{
	"env": {
		"DB_HOST": "@db_host",
		"DB_USER": "@db_user",
		"DB_PASSWORD": "@db_password",
		"DB_NAME": "@db_name",
		"DB_TABLE": "@db_table",
		"EMAIL": "@email",
		"EMAIL_PASSWORD": "@email_password"
	},
	"build": {
		"env": {
			"DB_HOST": "@db_host",
			"DB_USER": "@db_user",
			"DB_PASSWORD": "@db_password",
			"DB_NAME": "@db_name",
			"DB_TABLE": "@db_table",
			"EMAIL": "@email",
			"EMAIL_PASSWORD": "@email_password"
		}
	}
}
